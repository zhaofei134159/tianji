/**index.wxss**/
page {
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.scrollarea {
	flex: 1;
	overflow-y: hidden;
	background-image: url('data:image/webp;base64,');  
	background-size: cover; /* 根据需要调整背景大小 */  
	background-position: center; /* 背景图片居中显示 */  
	width: 100%; /* 根据需要设置 */  
	height: 100%; /* 根据需要设置 */  
}

/* 轮播 */  
.scrollarea .swiper-container {width: 100%;height: 235px;}
.swiper-container .swiper-list {height: 100%;}
.swiper-container .swiper-list .slide-image {width: 100%; height: 100%; display: block;}

/* 主体 */
.scrollarea .search-container {
    width: 80%;
    min-height: 200px;
    background-color: rgba(204,204,204,0.5);
    margin: 4% 4% 0px;
    border-radius: 5px;
    padding: 6%;
    color: black;
}
.search-container .section {width:100%; height:40px; line-height:40px; background: #fff; border-radius: 4px; margin-bottom: 10px; font-size: 15px;}
.search-container .section .section-key {display: inline-block; width:50%; height:100%;text-align:center;float:left;font-weight: bold; border-radius: 4px;}
.search-container .section .section-active {background: #4a4a4a;color: #fff;}
.search-container .section .section-text {display: inline-block; width:33%; height:100%;text-align:right;float:left; font-weight: bold;}
.search-container .section .section-val {display: inline-block; width:66%; height:100%;float:right;text-align:center;}
.search-container .section .section-botton {display: inline-block; width:100%; height:100%;text-align:center;float:left;font-weight: bold; color: #fff; background: #4f4c4c; border-radius: 4px; }
